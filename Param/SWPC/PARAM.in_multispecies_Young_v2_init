3.5			r2Shell

#BODY
T			UseBody
2.5			rBody     (in planet radius)
3.0			rCurrents (in planet radius)
7.5			BodyNDim  (/cc) H+
2.8			BodyNDim  (/cc) O+
25000.0			BodyTDim  (K)

#MULTIFLUIDIM
T			DoMultiFluidIMCoupling

#TIMESTEPPING
1			nStage
0.80			CflExlp

#OUTERBOUNDARY
outflow			TypeBc1
vary			TypeBc2
float			TypeBc3
float			TypeBc4
float			TypeBc5
float			TypeBc6

#INNERBOUNDARY
ionosphere			TypeBcInner

! Set the inner boundary density with the Young formula.
#YOUNGBOUNDARY
T			UseYoungBc
275.4			F107Young

#SOLARWINDFILE
T			UseSolarWindFile
IMF.dat			NameSolarWindFile

#REFRESHSOLARWINDFILE
T			DoReadAgain

#SCHEME
1			nOrder
Sokolov			TypeFlux

#SAVELOGNAME
F			IsLogNameNstep
T			IsLogNameEvent

#SAVEPLOTNAME
F			IsPlotNameNstep
F			IsPlotNameTime
T			IsPlotNameEvent

#PLOTFILENAME
date			TimeUnitMax

#SAVELOGFILE
T			DoSaveLo5Dgfile
VAR step date			StringLogfile
-1			DnSaveLogfile
5.0			DtSaveLogfile
dt rho mx my mz p bx by bz pmin pmax dst_sm dstflx ratioyoung cpcpn cpcps
3.0			StringRFlux

#SAVEPLOT
2			nPlotFiles
y=0 MHD idl             StringPlot
2500			DnSavePlot
-1.			DtSavePlot
-1.			DxSavePlot
z=0 MHD idl             StringPlot
2500			DnSavePlot
-1.			DtSavePlot
-1.			DxSavePlot

#DOAMR
T			DoAmr
100			DnAmr
-1.0			DtAmr
T			IsStrictAmr

! Satellite trajectories
#INCLUDE
SATELLITES.in

#MAGNETOMETER
magin_GEM.dat			NameMagInputFile
single			TypeFileOut
-1			DnOutput
1 min			DtOutput

#GEOMAGINDICES
180			nSizeKpWindow [min]
