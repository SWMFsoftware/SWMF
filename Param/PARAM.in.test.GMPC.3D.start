#ECHO
T

#DESCRIPTION
Run GM and PC in time accurate mode

#PLANET
NONE

#TIMEACCURATE
T			DoTimeAccurate

#PROGRESS
1			DnShowProgressShort
1			DnShowProgressLong


#COUPLE2TIGHT
GM			NameCompMaster
PC			NameCompSlave
T           DoCouple

#BEGIN_COMP GM ---------------------------------------------------------------


#IOUNITS
NONE			TypeIoUnit

#NORMALIZATION
NONE			TypeNormalization

#PLASMA
1.0             FluidMass [amu]
1.0             IonCharge [e]  
0.5             ElectronTemperatureRatio

#IOUNITS
NONE			NameIoUnits

#NORMALIZATION
NONE			NameNormalization

#UPDATECHECK
F                       UseUpdateCheck

#NONCONSERVATIVE
T			UseNonConservative

#CHECKGRIDSIZE
4				nI
4				nJ
4				nK
100				MinBlockALL

EQUATION
MHD with hyperbolic cleaning        NameEquation
9                                   nVar

USERMODULE
GEM and PIC coupling, G. Toth and L. Daldorff
1.00

#GRID
32			nRootBlockX
32			nRootBlockY
8			nRootBlockZ
-12.8			xMin
 12.8			xMax
-6.4			yMin
 6.4			yMax
-1.6			zMin
 1.6			zMax

#UNIFORMSTATE
1.0			rho
0.0			Ux
0.0			Uy
0.0         Uz	 
0.0			Bx
0.0			By
0.015		Bz
0.000157080	p

HYPERBOLICDIVB
T                       UseHyperbolicDivb
0.04                    SpeedHypDim
0.1                     HypDecay

#DIVB
T                       UseDivbSource
F                       UseDivbDiffusion
F                       UseProjection
F                       UseConstrainB

#PIC
T                       UsePic
2                       nGhostPic
0                       nOverlapPic
F			UseFileCoupling

#PICUNIT
 1.0                    xUnitPicSi
 1.0                    uUnitPicSi


PICREGION
1                       nPicRegion
-12.8  	                xMinCut
 12.6                    xMaxCut
-6.4                   yMinCut
+6.3                    yMaxCut
0.2                     DxPic
0.1                     DyPic

#PICREGION
1                       nPicRegion
-8.6  	                xMinCut
+8.6                    xMaxCut
-2.6                    yMinCut
+2.6                    yMaxCut
-1.0                    yMinCut
+1.0                    yMaxCut
0.2                     DxPic
0.1                     DyPic
0.1                     DyPic


#PLASMA
1.0             FluidMass [amu]
1.0             IonCharge [e]  
0.5             ElectronTemperatureRatio


#USERINPUTBEGIN -----------------

#GEM
0.2			Apert

#GEMPARAM
0.005			B0
0.000157080		Tp
1.0			CurrentSheetWidth

#GEMPERTURB
0.0			ExponentialWidth
0.0			ExponentialWidth
25.6			WaveLengthX
25.6			WaveLengthY

#USERINPUTEND ---------------------

#OUTERBOUNDARY
periodic		xMinBc
periodic		xMaxBc
float			yMinBc
float			yMaxBc
float			zMinBc
float			zMaxBc

#SCHEME
2			nOrder
Rusanov			TypeFlux
minmod			TypeLimiter
1.5			LimiterBeta

#TIMESTEPPING
2                       nStage
0.1                     CflExpl


HALLRESISTIVITY
T			UseHallResist (rest of parameters read only if true)
0.282095		HallFactor  (m/e = 1/sqrt(4*pi))
1.0			HallCmaxFactor

RESISTIVITY
T			UseResistivity
constant		TypeResistivity
0.01			Eta0Si

#SAVELOGFILE
T		DoSaveLogfile
var		StringLogfile
1		DnSaveLogfile
-1.0		DtSaveLogfile
dt rho mx my mz bx by bz e Pmin Pmax byflux
g

#SAVEINITIAL
T		        DoSaveInitial

#SAVEPLOT
3			nPlotfile
x=0 VAR idl_ascii       PlotString
4           	        DnSavePlot
-20.0             	DtSavePlot
0.0	        	DxSavePlot
rho ux uy uz bx by bz p jx jy jz absdivb
g
y=0 VAR idl_ascii        PlotString
4           	        DnSavePlot
-20.0             	DtSavePlot
0.0	        	DxSavePlot
rho ux uy uz bx by bz p jx jy jz absdivb
g
z=0 VAR idl_ascii        PlotString
4           	        DnSavePlot
-20.0             	DtSavePlot
0.0	        	DxSavePlot
rho ux uy uz bx by bz p jx jy jz absdivb
g


#END_COMP GM -----------------------------------------------------------------

#BEGIN_COMP PC ---------------------------------------------------------------

#ELECTRON
-25.0                                  Charge/mass ratio (qom)

#PARTICLES
3					Particles per cell in X
3					Particles per cell in Y
3					Particles per cell in Z

#SOLVER
1.0e-4					CGtol : CG solver stopping criterium tolerance
1.0e-4					GMREStol : GMRES solver stopping criterium tolerance
3					NiterMover : mover predictor corrector iteration

#SAVEPLOT
PC/plots				SaveDirName
0					FieldOutputCycle
0					ParticlesOutputCycle
0					DiagnosticsOutputCycle
4					FieldOutputCycleASCII
0					SatelliteOutputCycle

#END_COMP PC -----------------------------------------------------------------


#STOP
4			MaxIteration
-5000


5000.0			tSimulationMax

#END ############################################################
